
{
    "manifest_version": 3,
    "name": "ProductivityViz",
    "version": "0.1.0",
    "description": "Visualizes the time took to solve particular type of question",
   
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "cookies"
        
        
    ],
    "host_permissions": [
         "http://localhost:3001/*"
         
    ]
     ,
       "icons": { 
              
           "48": "Logo.png",
           "128": "Logo128.png"
           },
    "content_scripts":[
           {
                  "matches": ["https://codeforces.com/problemset/problem/*"],
                  "js":["SecondTimer.js"],
                  "css": ["styles.css"]
                  
           }
    ],
    "background": {
        "service_worker": "background.js"
       
    }
    
    
}
